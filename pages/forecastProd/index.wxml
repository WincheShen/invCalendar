<view class="page">
  <view wx:for="{{ready4Sell}}" class='item'>
    <view class='detail'>
      <view class='title'>{{item}}</view>
      <view class='onlineDate'>9月10日0点</view>
    </view>
    <view>
      <button id="111" bindtap="notice" size='mini' type='primary' disabled="{{disabled}}">通知我</button>
    </view>
  </view>
  <view class="query">
    <input type="text" placeholder="我最想买的产品..." confirm-type="done" bindconfirm="queryProduct" />
  </view>
</view>